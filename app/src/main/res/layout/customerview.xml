<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IT20199708.customerView"
    android:background="@drawable/background006">

    <ListView
        android:id="@+id/faqList"
        android:layout_width="384dp"
        android:layout_height="495dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="92dp"
        android:background="@drawable/layoutrounded"
        android:backgroundTint="#99FFFFFF"
        android:cacheColorHint="@color/colorPrimaryDarklevel2"
        android:foregroundTint="@color/colorPrimaryDarklevel2"
        android:listSelector="@color/colorPrimaryDarklevel2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtFaq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="FAQ"
        android:textColor="#F8F6F6"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/faqList"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>